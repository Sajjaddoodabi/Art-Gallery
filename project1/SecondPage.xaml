<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             BackgroundColor="{DynamicResource PageBackgroundColor}"
             x:Class="YourNamespace.SecondPage">


    <AbsoluteLayout>
        <Image Source="page1.jpg" Aspect="AspectFill" AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 1, 1" />

        <StackLayout>
		<Label Text="welcome to second page" FontSize="20" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
        <Button Text="Navigate to Main Page" Clicked="OnNavigateToMainPageBackButtonClicked"/>
        <Button Text="Navigate to Second Art" Clicked="OnNavigateToSecondPageButtonClicked"/>
    </StackLayout>
    </AbsoluteLayout>

</ContentPage>
